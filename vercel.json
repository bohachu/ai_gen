{
    "version": 2,
    "public": false,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "app.py"
        },
        {
            "src": "/docs",
            "dest": "app.py"
        },
        {
            "src": "/openapi.json",
            "dest": "app.py"
        },
        {
            "src": "/generate-code",
            "dest": "app.py"
        },
        {
            "src": "/static",
            "dest": "app.py"
        }
    ]
}
